{
  "name": "express-boilerplate",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "start": "node -r tsconfig-paths/register -r module-alias/register ./dist/src/index.js",
    "dev": "nodemon",
    "build": "tsc -p . && tsc-alias",
    "seed:dev": "ts-node -P tsconfig.scripts.json scripts/dev.seed.ts",
    "seed:prod": "ts-node -P tsconfig.scripts.json scripts/prod.seed.ts",
    "migrate:dev": "npx prisma migrate dev",
    "migrate": "npx prisma migrate deploy",
    "generate": "npx prisma generate",
    "studio": "npx prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.ts"
  },
  "_moduleAliases": {
    "@": "dist/src"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.5",
    "@types/nodemailer": "^6.4.17",
    "@types/response-time": "^2.3.8",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.13",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prisma": "^6.0.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.0",
    "express-rate-limit": "^7.5.0",
    "express-winston": "^4.2.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "nodemailer": "^6.9.16",
    "prom-client": "^15.1.3",
    "response-time": "^2.3.3",
    "uuid": "^11.0.5",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.0",
    "zod": "^3.24.1"
  }
}
